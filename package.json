{
  "name": "mongo-es",
  "version": "1.1.0",
  "description": "A MongoDB to Elasticsearch connector",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "start": "node ./dist/src/main.js",
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc -w & NODE_ENV=dev nodemon ./dist/src/main.js",
    "test": "tsc && ava ./dist/test/**-test.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com"
  },
  "bin": {
    "mongo-es": "./dist/src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jike-engineering/mongo-es.git"
  },
  "author": "",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/jike-engineering/mongo-es/issues"
  },
  "homepage": "https://github.com/jike-engineering/mongo-es#readme",
  "dependencies": {
    "elasticsearch": "13.2.0",
    "lodash": "4.17.4",
    "mongodb": "2.2.30",
    "rx": "4.1.0"
  },
  "devDependencies": {
    "@types/elasticsearch": "5.0.14",
    "@types/lodash": "4.14.71",
    "@types/mongodb": "2.2.9",
    "@types/rx": "4.1.1",
    "ava": "0.21.0",
    "nodemon": "1.11.0",
    "source-map-support": "0.4.15",
    "typescript": "2.4.2"
  }
}
